<assembly
	xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.3"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.3 http://maven.apache.org/xsd/assembly-1.1.3.xsd">
	<id>batch</id>
	<formats>
		<format>zip</format>
	</formats>
	<fileSets>
		<fileSet>
			<directory>src/main/resources/batch</directory>
			<outputDirectory>batch</outputDirectory>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/ini</directory>
			<outputDirectory>ini</outputDirectory>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/dataIntegration</directory>
			<outputDirectory>ini/dataIntegration</outputDirectory>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/jobs</directory>
			<outputDirectory>jobs</outputDirectory>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/transformations</directory>
			<outputDirectory>transformations</outputDirectory>
		</fileSet>
		<fileSet>
			<directory>target/libKettle</directory>
			<outputDirectory>libKettle</outputDirectory>
		</fileSet>
		<fileSet>
			<directory>target/libBatch</directory>
			<outputDirectory>libBatch</outputDirectory>
		</fileSet>
	</fileSets>
</assembly>